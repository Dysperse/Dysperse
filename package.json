    "postinstall": "if [ \"$EAS_BUILD_PLATFORM\" = \"android\" ]; then npm uninstall react-native-autogrow-textinput; fi && npx patch-package",
    "build:android:dev": "eas build --profile development --platform android",
    "build:android:prev": "eas build --profile preview --platform android",
    "build:android:prod": "npm i -g eas-cli && eas build --profile production --platform android",
    "build:ios:dev": "eas build --profile development --platform ios",
    "build:ios:prev": "eas build --profile preview --platform ios",
    "build:ios:prod": "npm i -g eas-cli && eas build --profile production --platform ios",
    "build:web": "node ./minify-web.js && expo export -p web && npm install workbox-cli --global && workbox injectManifest workbox-config.js",
    "build:electron": "expo export -p web --output-dir ./electron/src/dist",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build-local": "eas build --platform android --local"
